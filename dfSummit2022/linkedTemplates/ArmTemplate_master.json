{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfSummit2022"
		},
		"andyweather_sqltable_LinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'andyweather_sqltable_LinkedService'"
		},
		"Summit2022AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kvSummit20221.vault.azure.net/"
		},
		"andyweather_csv_blob_linkedservice_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'andyweather_csv_blob_linkedservice'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfSummit2022_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"andyweather_sqltable_LinkedService_connectionString": {
						"value": "[parameters('andyweather_sqltable_LinkedService_connectionString')]"
					},
					"Summit2022AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('Summit2022AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"andyweather_csv_blob_linkedservice_sasUri": {
						"value": "[parameters('andyweather_csv_blob_linkedservice_sasUri')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}